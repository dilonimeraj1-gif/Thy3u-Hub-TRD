	Description = "get all gems and coins to win automatically. ",
	CurrentValue = false,
    	Callback = function(Value)
       collectcoinsloop = Value
        while collectcoinsloop == true do
            task.wait(0.00001)
            for i, v in pairs(workspace:GetDescendants()) do
